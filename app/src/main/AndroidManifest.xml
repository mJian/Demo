<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maj.demo">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AnimActivity" />
        <activity android:name=".activity.TextViewForMarquee" />
        <activity
            android:name=".activity.LockScreenActivity"
            android:theme="@style/Dialog" />
        <activity android:name=".activity.NormalActivity" />
        <activity android:name=".activity.RudeActivity" />
        <activity android:name=".activity.RecyclerViewActivity" />
        <activity android:name=".activity.MultiPathActivity" />
        <activity android:name=".activity.VideoPlayActivity"></activity>
    </application>

</manifest>